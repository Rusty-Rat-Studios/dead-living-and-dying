; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Dead Living and Dying"
run/main_scene="res://src/ui/menus/main_menu.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://src/icon.svg"

[autoload]

SignalBus="*res://src/autoloads/signal_bus.gd"
PlayerHandler="*res://src/autoloads/player_handler.gd"
Focus="*res://src/autoloads/focus.gd"
CollisionBit="*res://src/autoloads/collision_bit.gd"
Utility="*res://src/autoloads/utility.gd"
CommandParser="*res://src/autoloads/command_parser.gd"
ShrineManager="*res://src/autoloads/shrine_manager.gd"
RNG="*res://src/autoloads/rng.gd"
SpawnerManager="*res://src/autoloads/spawner_manager.gd"
UIDevice="*res://src/autoloads/ui_device.gd"
DebugMenu="*res://addons/debug_menu/debug_menu.tscn"
KeyItemHandler="*res://src/autoloads/key_item_handler.gd"

[debug]

gdscript/warnings/untyped_declaration=2
gdscript/warnings/inferred_declaration=2

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/stretch/scale=0.8

[editor_plugins]

enabled=PackedStringArray("res://addons/debug_menu/plugin.cfg", "res://addons/gut/plugin.cfg")

[filesystem]

import/blender/enabled=false

[global_group]

ghosts=""
rooms=""

